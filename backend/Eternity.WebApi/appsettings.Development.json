{
  "JwtSettings": {
    "SecretKey": "dev-secret-key-at-least-32-characters-long-change-in-production",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 30
  },
  "CookieSettings": {
    "SameSiteMode": "Lax",
    "SecurePolicy": "SameAsRequest"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:4200"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  }
}
